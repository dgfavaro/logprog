                <then>
                    <output expression="&quot;Valor Da Tarifa Alterado:(distancia*vvar+vfixo)*1.2&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Valor Da Tarifa:distancia*vvar+vfixo&quot;" newline="True"/>
                </else>
            </if>
        </body>
