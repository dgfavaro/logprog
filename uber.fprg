    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <assign variable="vfixo" expression="10"/>
            <assign variable="vvar" expression="2"/>
            <output expression="&quot;Insira a distancia percorrida: &quot;" newline="True"/>
            <input variable="distancia"/>
            <output expression="&quot;Esta corrida sera faita durante a fasta do peao? 0 ou 1&quot;" newline="True"/>
            <input variable="fdp"/>
